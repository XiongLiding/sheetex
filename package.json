{
  "name": "sheetex",
  "version": "1.0.4",
  "description": "12KB Lightweight Excel Export Tool with Full Style Support, Works Out-of-the-Box for Browsers & Node.js",
  "type": "module",
  "homepage": "https://github.com/XiongLiding/sheetex",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/bundle.cjs",
    "import": "./dist/bundle.mjs"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=dist/bundle.cjs",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/bundle.mjs",
    "build:all": "npm run build && npm run build:min",
    "build:min": "npm run build:cjs:min && npm run build:esm:min",
    "build:cjs:min": "esbuild src/index.ts --bundle --minify --sourcemap --format=cjs --outfile=dist/bundle.min.cjs",
    "build:esm:min": "esbuild src/index.ts --bundle --minify --sourcemap --format=esm --outfile=dist/bundle.min.mjs",
    "build:types": "tsc --emitDeclarationOnly",
    "postbuild": "npm run build:types"
  },
  "keywords": [],
  "author": "XiongLiding",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "fflate": "^0.8.2",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "esbuild": "^0.25.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
